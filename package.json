  "scripts": {
    "build": "yarn build:clean && yarn build:package",
    "build:clean": "rm -rf build/ && mkdir -p build/",
    "build:package": "NODE_ENV=production webpack",
    "serve": "NODE_ENV=development webpack-dev-server",
    "serve:design": "NODE_ENV=development webpack-dev-server --config webpack.design.config.js",
    "scrivito": "yarn scrivito:build && yarn scrivito:copy && yarn scrivito:commit",
    "scrivito:build": "cd ../rails_connector && bundle && bundle exec rake package:js",
    "scrivito:copy": "rm -rf vendor/scrivito && mkdir -p vendor/scrivito && cp ../rails_connector/pkg/js/scrivito/* vendor/scrivito",
    "scrivito:commit": "git add vendor/scrivito && git commit -m \"[sdk] Updated Scrivito SDK to version `cd ../rails_connector && git describe`\""
  },
  "dependencies": {
    "moment": "^2.18.1",
    "react-addons-shallow-compare": "^15.6.0",
    "react-dates": "^12.4.0",
    "wu": "^2.1.0"
  }
}
